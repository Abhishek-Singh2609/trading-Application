{
    "version": 2,
    "projects": [
      {
        "name": "frontend",
        "root": "frontend",
        "builds": [
          {
            "src": "frontend/package.json",
            "use": "@vercel/static-build",
            "config": {
              "distDir": "frontend/build"
            }
          }
        ]
      },
      {
        "name": "backend",
        "root": "backend",
        "builds": [
          {
            "src": "backend/index.js",
            "use": "@vercel/node"
          }
        ]
      },
      {
        "name": "dashboard",
        "root": "dashboard",
        "builds": [
          {
            "src": "dashboard/package.json",
            "use": "@vercel/static-build",
            "config": {
              "distDir": "dashboard/build"
            }
          }
        ]
      }
    ]
  }
  